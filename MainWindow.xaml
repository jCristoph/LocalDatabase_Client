<Window x:Class="LocalDatabase_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocalDatabase_Client"
        mc:Ignorable="d"
        Title="Klient" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBoxLogin" HorizontalAlignment="Left" Height="26" Margin="88,360,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="143"/>
        <TextBox x:Name="textBoxPassword" HorizontalAlignment="Left" Height="26" Margin="371,360,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="143"/>
        <Button x:Name="LoggingButton" Content="Loguj" HorizontalAlignment="Left" Margin="651,359,0,0" VerticalAlignment="Top" Width="75" Click="LoggingButton_Click"/>
        <ListBox x:Name="listBox" Margin="88,67,212,133"/>
        <Button Content="Wyślij plik" HorizontalAlignment="Left" Margin="651,67,0,0" VerticalAlignment="Top" Width="75" Click="SendFileButton"/>
        <Button Content="Pobierz plik" HorizontalAlignment="Left" Margin="651,144,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.173,0.2" Click="DownloadFileButton"/>
        <Button Content="Usuń plik" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="651,209,0,0" Click="DeleteFileButton"/>
    </Grid>
</Window>
